<aura:component implements="forceCommunity:availableForAllPageTypes" controller="AuraShoppingCartController" access="global">

    <aura:attribute name="items" type="OrderItem[]" access="private"/>
    <aura:handler name="init" value="{!this}" action="{!c.Initialize}"/>
    <lightning:button label="TestPrint" onclick="{!c.TestPrint}"/>
    

    <aura:iteration items="{!v.items}" var="item" indexVar="index">
        <c:productCard onremove="{!c.HandleRemove}"></c:productCard>
    </aura:iteration>

</aura:component>	
